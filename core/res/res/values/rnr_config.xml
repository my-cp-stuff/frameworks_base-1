<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Whether the device is using Renoir's Accent colors -->
    <bool name="config_isUsingRenoirAccent">false</bool>
    <!-- Whether to pick the current accent style
        - 0: Primary color accent
        - 1: Secondary color accent
        - 2: Myterial color accent -->
    <integer name="config_isUsingRenoirAccentType">-1</integer>
    <!-- Whether the device is using Renoir's UI Style colors -->
    <bool name="config_isUsingRenoirUIStyle">false</bool>
    <!-- Whether to pick the current UI style
        - 0: Myterial UI Style
        - 1: Transparent Tinted UI Style-->
    <integer name="config_isUsingRenoirUIStyleType">-1</integer>
</resources>
