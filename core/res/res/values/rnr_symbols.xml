<resources>
	<!-- Renoir color values -->
    <!-- Accent color resources -->
    <java-symbol type="color" name="rnr_accent_primary_color_accent" />
    <java-symbol type="color" name="rnr_accent_primary_color_gradient" />
    <!-- Accent alt color resources -->
    <java-symbol type="color" name="rnr_accent_secondary_color_accent" />
    <java-symbol type="color" name="rnr_accent_secondary_color_gradient" />
    <!-- Accent myterial color resources -->
    <java-symbol type="color" name="rnr_accent_myterial_color_accent" />
    <java-symbol type="color" name="rnr_accent_myterial_color_gradient" />

    <!-- Myterial UI Style color resources -->
    <!-- Light mode -->
    <java-symbol type="color" name="rnr_myterial_background_color_light" />
    <java-symbol type="color" name="rnr_myterial_primary_color_light" />
    <!-- Dark mode -->
    <java-symbol type="color" name="rnr_myterial_background_color_dark" />
    <java-symbol type="color" name="rnr_myterial_primary_color_dark" />
    <!-- Tinted Transparent UI Style color resources -->
    <!-- Light mode -->
    <java-symbol type="color" name="rnr_tinted_background_color_light" />
    <java-symbol type="color" name="rnr_tinted_primary_color_light" />
    <!-- Dark mode -->
    <java-symbol type="color" name="rnr_tinted_background_color_dark" />
    <java-symbol type="color" name="rnr_tinted_primary_color_dark" />

    <!-- Configs -->
    <!-- Whether the device is using Renoir's Accent colors -->
    <java-symbol type="bool" name="config_isUsingRenoirAccent" />
    <java-symbol type="integer" name="config_isUsingRenoirAccentType" />
    <!-- Whether the device is using Renoir's UI Style colors -->
    <java-symbol type="bool" name="config_isUsingRenoirUIStyle" />
    <java-symbol type="integer" name="config_isUsingRenoirUIStyleType" />
</resources>
